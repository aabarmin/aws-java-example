{
  "family": "app-service-discovery",
  "networkMode": "awsvpc",
  "containerDefinitions": [{
    "name": "app-service-discovery-container",
    "image": "835357144480.dkr.ecr.us-east-1.amazonaws.com/app-discovery-server:latest",
    "portMappings": [{
      "containerPort": 8888,
      "hostPort": 8888,
      "protocol": "tcp"
    }],
    "essential": true,
    "logConfiguration": {
      "logDriver": "awslogs",
      "options": {
        "awslogs-group": "/ecs/app-service-discovery",
        "awslogs-region": "us-east-1",
        "awslogs-stream-prefix": "ecs"
      }
    }
  }],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "256",
  "memory": "512"
}